{
  "name": "instaelum-ssr-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.4.0",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon --exec babel-node devtools/server",
    "start:dev": "cross-env NODE_ENV=development babel-node devtools/server",
    "dev": "react-scripts start",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config devtools/production/webpack.config.js",
    "moveStaticFiles": "cross-env cp -a ./public/. ./dist && rm -rf ./dist/index.html"
  },
  "devDependencies": {
    "react-scripts": "1.1.0",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "img-loader": "^2.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "style-loader": "^0.20.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}
